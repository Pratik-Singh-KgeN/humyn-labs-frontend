{
  "name": "humyn-labs-frontend",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.28.0",
  "type": "module",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "generate": "node packages/generator/src/cli.ts",
    "prepare": "husky install",
    "typecheck": "turbo run typecheck",
    "lint:eslint": "eslint --ext .ts,.tsx,.js,.jsx .",
    "format:prettier": "prettier --write \"**/*.{ts,tsx,js,jsx,css,scss,json,md}\"",
    "precommit": "pnpm typecheck && pnpm lint:eslint && pnpm format:prettier",
    "test": "turbo run test",
    "test:watch": "turbo run test --watch"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@testing-library/jest-dom": "^6",
    "@testing-library/react": "^16",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.9",
    "@types/minimist": "^1.2.5",
    "@types/node": "^25.0.5",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "fs-extra": "^11.3.3",
    "globals": "^17.0.0",
    "husky": "^8.0.0",
    "jsdom": "^22.1.0",
    "lint-staged": "^16.2.7",
    "msw": "^2.12.7",
    "next": "^16.1.1",
    "prettier": "^3.7.4",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "ts-node": "^10.9.2",
    "turbo": "^2.7.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0",
    "vite": "^7.3.1",
    "vitest": "^4"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,json,md}": [
      "prettier --write"
    ]
  }
}
